#. type: TP
#: ../man/slabtop.1:75
#, no-wrap
msgid "B<Q>"
msgstr "B<Q>"

#. type: Plain text
#: ../man/slabtop.1:78
msgid "Quit the program."
msgstr "Avsluta programmet."

#. type: TP
#: ../man/slabtop.1:79
#, no-wrap
msgid "I</proc/slabinfo>"
